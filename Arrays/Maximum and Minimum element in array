class Solution {
    public Pair<Long, Long> getMinMax(int[] arr) {
        // Code Here
        int maximum=arr[0];
        int minimum = arr[0];
        for(int i=0;i<arr.length;i++){
            if(arr[i]>maximum){
                maximum = arr[i];
            }
            else if(arr[i]<minimum){
                minimum = arr[i];
            }
        }
        return new Pair(minimum,maximum);
    }
}
